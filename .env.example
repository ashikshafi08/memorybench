# LLM API Keys for Evaluation
# Required: At least one of these must be set based on your evaluation model choice

# Anthropic (Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI (GPT models)
OPENAI_API_KEY=your_openai_api_key_here

# OpenRouter (Unified access to multiple models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Google (Gemini models)
GOOGLE_API_KEY=your_google_api_key_here

# Voyage AI (Required for AQRAG and ContextualRetrieval embeddings)
VOYAGE_API_KEY=your_voyage_api_key_here

# Optional: Hosted Memory Provider API Keys
# Only needed if testing hosted providers (Supermemory, Mem0, etc.)

# Supermemory
SUPERMEMORY_API_KEY=your_supermemory_api_key_here
SUPERMEMORY_API_URL=https://api.supermemory.ai/v1

# Mem0
MEM0_API_KEY=your_mem0_api_key_here
MEM0_BASE_URL=https://api.mem0.ai/v1

# Database Configuration (Optional)
# PostgreSQL connection for AQRAG local provider
# If not set, defaults to local PostgreSQL instance

DATABASE_URL=postgresql://memorybench:memorybench@localhost:5433/memorybench
